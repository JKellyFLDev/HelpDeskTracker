<Application x:Class="LeafFilter.HelpDesk.TrackerApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
             xmlns:views="clr-namespace:LeafFilter.HelpDesk.TrackerApp.View"
             xmlns:viewsTicket="clr-namespace:LeafFilter.HelpDesk.TrackerApp.View.TicketView"
             xmlns:viewsModels="clr-namespace:LeafFilter.HelpDesk.TrackerApp.ViewModel"
             xmlns:viewsModelTicket="clr-namespace:LeafFilter.HelpDesk.TrackerApp.ViewModel.TicketViewModel">
    <!--StartupUri="View/MainWindowView.xaml"-->
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <materialDesign:BundledTheme BaseTheme="Light" PrimaryColor="Green" SecondaryColor="Cyan" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.Defaults.xaml" />
                <ResourceDictionary Source="/LeafFilter.HelpDesk.TrackerApp;component/Styling/LFTheme.Defaults.xaml"/>
            </ResourceDictionary.MergedDictionaries>

            <DataTemplate DataType="{x:Type viewsModelTicket:TicketListViewModel}">
                <viewsTicket:TicketListView />
            </DataTemplate>
        </ResourceDictionary>
    </Application.Resources>
</Application>

    